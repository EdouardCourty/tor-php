{
    "name": "ecourty/tor-php",
    "description": "A Symfony-based HTTP client and ControlPort client for interacting with the Tor network in PHP.",
    "type": "library",
    "keywords": [
        "Tor",
        "HTTP Client"
    ],
    "require": {
        "php": ">= 8.3",
        "symfony/http-client": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.71",
        "symfony/var-dumper": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TorPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TorPHP\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edouard Courty",
            "email": "edouard.courty2@gmail.com",
            "homepage": "https://github.com/EdouardCourty"
        }
    ]
}
